%{
    #include "slang.hpp"
    #include <iostream>
%}

%token FUNC PRINT RETURN OPENBLOCK CLOSEBLOCK INT
%token VAR INT_NUMBER FLOAT_NUMBER IDENTIFIER STRING

%left '|' '&' '^'
%left '+' '-'
%left '*' '/'
%nonassoc UMINUS
%right '~'
//%expect 1

%nonassoc IF THEN
%nonassoc ELSE

%%

program : FUNC {

}

%%

int yyerror(const char* error) {
    std::cout << error << " on line " << yylineno << std::endl;
    exit(EXIT_FAILURE);
}